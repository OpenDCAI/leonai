{
  "api": {
    "model": "claude-sonnet-4-5-20250929",
    "temperature": 0.3,
    "context_limit": 150000,
    "queue_mode": "collect",
    "block_dangerous_commands": true,
    "block_network_commands": true
  },
  "memory": {
    "pruning": {
      "enabled": true,
      "soft_trim_chars": 5000,
      "hard_clear_threshold": 15000,
      "protect_recent": 5,
      "trim_tool_results": true
    },
    "compaction": {
      "enabled": true,
      "reserve_tokens": 20000,
      "keep_recent_tokens": 30000,
      "min_messages": 25
    }
  },
  "tools": {
    "filesystem": {
      "enabled": true,
      "tools": {
        "read_file": { "enabled": true, "max_file_size": 20971520 },
        "write_file": false,
        "edit_file": false,
        "multi_edit": false,
        "list_dir": true
      }
    },
    "search": { "enabled": true, "max_results": 100 },
    "command": { "enabled": false },
    "web": {
      "enabled": true,
      "timeout": 30,
      "tools": {
        "web_search": { "enabled": true, "max_results": 10 },
        "read_url_content": { "enabled": true },
        "view_web_content": true
      }
    }
  },
  "system_prompt": "You are a research expert. Focus on gathering comprehensive information, analyzing sources critically, and providing well-reasoned insights with citations."
}
