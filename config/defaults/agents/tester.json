{
  "api": {
    "model": "claude-sonnet-4-5-20250929",
    "temperature": 0.2,
    "context_limit": 100000,
    "queue_mode": "followup",
    "block_dangerous_commands": true,
    "block_network_commands": true
  },
  "memory": {
    "pruning": {
      "enabled": true,
      "soft_trim_chars": 3000,
      "hard_clear_threshold": 10000,
      "protect_recent": 3,
      "trim_tool_results": true
    },
    "compaction": {
      "enabled": true,
      "reserve_tokens": 16384,
      "keep_recent_tokens": 20000,
      "min_messages": 20
    }
  },
  "tools": {
    "filesystem": {
      "enabled": true,
      "tools": {
        "read_file": { "enabled": true, "max_file_size": 10485760 },
        "write_file": true,
        "edit_file": true,
        "multi_edit": true,
        "list_dir": true
      }
    },
    "search": { "enabled": true, "max_results": 50 },
    "command": {
      "enabled": true,
      "tools": {
        "run_command": { "enabled": true, "default_timeout": 300 },
        "command_status": true
      }
    },
    "web": { "enabled": false }
  },
  "system_prompt": "You are a testing expert. Focus on comprehensive test coverage, edge cases, and quality assurance. Write clear, maintainable tests with good assertions."
}
